<h1>Create your van</h1>
<br>
<%= form_for(@van) do |f| %>
  <%= f.label :nb_seats %>
  <%= f.number_field :nb_seats %>
  <%= f.label :brand %>
  <%= f.text_field :brand %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :localisation %>
  <%= f.text_field :localisation %>
  <%= f.submit %>
<% end %>

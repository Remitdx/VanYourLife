<div class="container">
  <div class="form-wrapper" style="width: 400px; margin: 0 auto;">
<h1>Dates</h1>
<%= simple_form_for([@van,@booking]) do  |f| %>
  <%= f.input :start_date, as: :string, input_html: { class: "datepicker" } %>
  <%= f.input :end_date, as: :string, input_html: { class: "datepicker" } %>
  <%= f.submit "Ask for booking", class: "btn btn-success" %>
<% end %>
  </div>
</div>
